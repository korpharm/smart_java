package com.ict.edu;

public class Ex06 {
public static void main(String[] args) {
	//다중 for문:for문 안에 for문이 존재
	for (int i = 1; i <4; i++) {
		for (int j = 1; j <6; j++) {
			//System.out.print("i="+i);
			//System.out.println(",j="+j);
			//System.out.println("i="+i+",j="+j);
			//System.out.println(+i+j);
			System.out.println(i+j);
		}
	}
	
}
}
